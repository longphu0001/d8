{
    "name": "drupal-composer/drupal-project",
    "description": "Project template for Drupal 8 projects with composer",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "php": ">=5.6",
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.6.5",
        "drupal-composer/drupal-scaffold": "^2.5",
        "drupal/addtoany": "^1.10",
        "drupal/admin_toolbar": "^1.26",
        "drupal/adminimal_admin_toolbar": "^1.9",
        "drupal/adminimal_theme": "^1.4",
        "drupal/advagg": "^3.5",
        "drupal/amp": "^3.0",
        "drupal/back_to_top": "^1.0",
        "drupal/backup_migrate": "^4.0",
        "drupal/cdn": "^3.2",
        "drupal/coffee": "^1.0",
        "drupal/colorbox": "^1.4",
        "drupal/context": "^4.0",
        "drupal/core": "^8.6.0",
        "drupal/custom_meta": "^1.0",
        "drupal/dropzonejs": "^2.0",
        "drupal/editor_advanced_link": "^1.4",
        "drupal/embed": "^1.0",
        "drupal/entity_browser": "^2.1",
        "drupal/eu_cookie_compliance": "^1.2",
        "drupal/fb_likebox": "^2.0",
        "drupal/features": "^3.8",
        "drupal/focal_point": "^1.0",
        "drupal/fontawesome": "^2.11",
        "drupal/google_analytics": "^3.0",
        "drupal/honeypot": "^1.29",
        "drupal/hreflang": "^1.1",
        "drupal/image_widget_crop": "^2.2",
        "drupal/imagemagick": "^3.0",
        "drupal/insert": "^2.0",
        "drupal/lang_dropdown": "^2.0",
        "drupal/languageicons": "^1.0",
        "drupal/lazyloader": "^1.0",
        "drupal/linkit": "^5.0",
        "drupal/mailchimp": "^1.8",
        "drupal/mailsystem": "^4.1",
        "drupal/menu_block": "^1.5",
        "drupal/menu_breadcrumb": "^1.8",
        "drupal/metatag": "^1.7",
        "drupal/paragraphs": "^1.5",
        "drupal/pathauto": "^1.3",
        "drupal/rrssb": "^2.0",
        "drupal/schema_metatag": "^1.3",
        "drupal/search404": "^1.0",
        "drupal/search_api": "^1.11",
        "drupal/search_autocomplete": "^1.1",
        "drupal/seckit": "^1.1",
        "drupal/sharethis": "^2.0",
        "drupal/simple_sitemap": "^3.0",
        "drupal/sitemap": "^1.5",
        "drupal/slick": "^2.0",
        "drupal/slick_extras": "^1.0",
        "drupal/slick_views": "^2.0",
        "drupal/superfish": "^1.3",
        "drupal/tagclouds": "^1.0",
        "drupal/twig_tweak": "^2.1",
        "drupal/video_embed_field": "^2.0",
        "drupal/videojs": "^1.0",
        "drupal/views_accordion": "^1.2",
        "drupal/views_bulk_operations": "^2.4",
        "drupal/views_conditional": "^1.0",
        "drupal/views_infinite_scroll": "^1.5",
        "drupal/visitors": "^2.0",
        "drupal/webform": "^5.1",
        "drupal/yoast_seo": "^2.0",
        "vlucas/phpdotenv": "^2.4",
        "webflo/drupal-finder": "^1.0.0",
        "webmozart/path-util": "^2.3",
        "zaporylie/composer-drupal-optimizations": "^1.0"
    },
    "require-dev": {
        "drush/drush": "^9.0.0",
        "drupal/devel": "^2.0",
        "drupal/console": "^1.0.2",
        "drupal/seo_checklist": "^4.1",
        "drupal/checklistapi": "^1.8",
        "webflo/drupal-core-require-dev": "^8.6.0"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": ["load.environment.php"]
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/{$name}": ["type:drupal-drush"]
        },
        "drupal-scaffold": {
            "initial": {
                ".editorconfig": "../.editorconfig",
                ".gitattributes": "../.gitattributes"
            }
        }
    }
}
